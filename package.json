{
  "name": "hh-school-react",
  "description": "Воробьева В.О.",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "yarn eslint && yarn prettier && yarn stylelint",
    "eslint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "eslint:fix": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "prettier": "prettier --write .",
    "prettier:check": "prettier --check .",
    "lint:fix": "eslint --fix .",
    "stylelint": "stylelint src/**/*.css",
    "preview": "vite preview",
    "prepare": "husky install",
    "test": "echo \"Error: no test specified\" && exit 1",
    "deploy": "yarn build && gh-pages -d dist"
  },
  "dependencies": {
    "octokit": "^3.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@hh.ru/eslint-config": "^15.1.1",
    "@hh.ru/eslint-plugin-import-rules": "^1.5.2",
    "@hh.ru/stylelint-config-hh": "^10.2.1",
    "@types/babel__core": "^7",
    "@types/eslint": "^7",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "5.51.0",
    "@typescript-eslint/parser": "5.51.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "7.28.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-dollar-sign": "1.0.2",
    "eslint-plugin-etc": "1.5.2",
    "eslint-plugin-import": "2.23.2",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "gh-pages": "^6.1.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.0",
    "prettier": "2.8.7",
    "stylelint": "~13.13.1",
    "stylelint-order": "~4.1.0",
    "stylelint-scss": "~3.19.0",
    "typescript": "4.9.5",
    "vite": "^5.0.7"
  }
